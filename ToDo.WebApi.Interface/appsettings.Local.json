{
  "ConnectionStrings": {
    "DefaultDatabaseConnection": "Host=localhost;Port=5432;Database=testedb;Username=admin;Password=secrepassword!234"
  },
  "Randomizer": {
    "RandomizerSeed": "8574",
    "DefaultQuantityGeneration": "100"
  },
  "JwtSettings": {
    "Secret": "ultra-mega-incredulously-secret-key-fr",
    "ExpiryMinutes": "6000",
    "Issuer": "Local-Todo-webapi"
  },
  "AuthSettings": {
    "Secret": "ultra-mega-incredulously-secret-key-fr"
  },
  "Serilog": {
    "Using": [ "Serilog.Sinks.Console" ],
    "MinimumLevel": {
      "Default": "Debug",
      "Override": {
        "Microsoft": "Information",
        "System": "Information"
      }
    },
    "Enrich": [
      "FromLogContext",
      "WithProcessId",
      "WithHttpContext",
      "WithClientAgent",
      "WithExceptionStackTraceHash",
      "WithCorrelationId",
      "WithEnrichedProperties"
    ],
    "WriteTo": [
      {
        "Name": "Console",
        "Args": {
          "outputTemplate": "===> {Timestamp:HH:mm:ss} [{Level}] {Message}{NewLine}{Exception}"
        }
      }
    ]
  },
  "AllowedHosts": "*"
}
